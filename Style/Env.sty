\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{Env}

\newtcolorbox{IN}{
    colback=green!15,
    oversize=2pt,
    colframe=blue,
    before skip=\topsep,
    after skip=\topsep,
    title=Important Notes,
    colbacktitle=blue!75,
    coltitle=white,
    enhanced,
    attach boxed title to top center={yshift=-3mm,yshifttext=-1mm},
  }

\newtcolorbox{stbox}[2][]{
colback=yellow!20,
oversize=2pt,
colframe=blue!75!black,
colbacktitle=blue!75!black,
coltitle=white,
enhanced,
attach boxed title to top left={xshift=3mm,yshift=-3mm,yshifttext=-1mm},
title={#2},#1}